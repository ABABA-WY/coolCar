.lic-tmpl{
    width: 620rpx;
    height: 460;
    position: relative;
    display: flex;
    justify-content: center;
    align-items: center;
}
.lic-tmpl.image{
    opacity: 0.3;
    position: absolute;
    width: 100%;
    height: 600rpx;
}
.cuIcon-camera.lg.text-gray{
    font-size: 120rpx;
    position: relative;
    top: 50rpx;
}
.instruction{
    position: absolute;
    bottom: -22rpx;
    top: 170rpx;
}
.stretch{
    width: 700%;
    height: 500rpx;
}
.lic-form{
    width: 620rpx;
}
.cu-form-group{
    min-width: calc(4em+30rpx);
    margin-top: 5rpx;
}
.padding.flex.flex-direction{
    width: 450rpx;
}